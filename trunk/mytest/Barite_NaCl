# Barite solubility, in agreement with Templeton, 1960
DATABASE ../database/vreeqc.dat
SOLUTION 1
 pH 7 charge
 -temp 50
EQUILIBRIUM_PHASES
 Barite
REACTION
 NaCl 1; 0 5*0.1 9*0.5
INCREMENTAL_REACTIONS true
USER_GRAPH
 axis_titles "NaCl / M" "Barite solubility / M"
 10 plot_xy tot("Na"), tot("Ba"), color = Red, symbol = None
END
